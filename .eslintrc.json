{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "rules": {
      "indent": ["error", 4],
      "react/jsx-indent": ["error", 4],
      "react/prefer-es6-class": ["error", "never"],
      "quotes": ["error", "double", {"avoidEscape": true}]
    }
}
